            "App\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.5",
        "monolog/monolog": "^3.10",
        "slim/slim": "^4.15",
        "slim/psr7": "^1.8",
        "php-di/php-di": "^7.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.92",
        "symfony/var-dumper": "^7.4",
        "rector/rector": "^2.3"
    },
    "config": {
        "process-timeout": 900
    }
}
